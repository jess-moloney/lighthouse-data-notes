    "import plotly.express as px\n",
    "df = px.data.gapminder().query(\"year==2007\")\n",
    "fig = px.scatter_geo(df, locations=\"iso_alpha\", color=\"continent\",\n",
    "                     hover_name=\"country\", size=\"lifeExp\",\n",
    "                     projection=\"natural earth\")\n",
    "fig.show()"
   ]
